type User {
  id: Int! @id 
  userName: String! @unique
  email : String! @unique
  firstName: String @default(value: "")
  lastname: String
  bio: String
  followers: [User!]! @relation(name: "FollowRelation")
  following: [User!]! @relation(name: "FollowRelation")
  post: [Post!]!
  likes: [Like!]!
  comments: [Comment!]!
}

type Post {  
  id: Int! @id 
  location:String!
  caption: String!
  user: User!
  files: [File!]!
  likes: [Like!]!
  comments: [Comment!]!
}

type Like {
  id: Int! @id 
  user: User!
  post: Post!
}

type Comment {
  id: Int! @id 
  text: String!
  user: User!
  post: Post!
}

type File {
  id: Int! @id 
  url: String!
  post: Post!
}

